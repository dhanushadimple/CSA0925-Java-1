import java.util.*;
class import java.util.*;
class Reversestring
{
public static void main(String args[])
{
String str;
String rev=" ";
char ch;
Scanner sc=new Scanner(System.in);
System.out.println("enter the string");
str=sc.nextLine();
int n=str.length();
for(int i=n-1;i>=0;i--)
{
 ch=str.charAt(i);
rev=rev+ch;
}
System.out.println("reverse string:" +rev);
}
}
{
public static void main(String args[])
{
String str;
